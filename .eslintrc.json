{
    "extends": ["eslint:recommended", "airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error",
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "eqeqeq": ["error", "always"],
        "no-unused-expressions":"error"
    }
}
